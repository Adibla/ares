{
  "db":  {
    "connection": {
      "MONGODB":{
        "host": "DB_MONGODB_HOST",
        "port": "DB_MONGODB_PORT",
        "user": "DB_MONGODB_USER",
        "password": "DB_MONGODB_PASS",
        "database": "DB_MONGODB_NAME",
        "authMechanism": "AUTH_MONGODB_MECHANISM",
        "authSource": "AUTH_MONGODB_SOURCE"
      },
      "MYSQL":{
        "host": "DB_MYSQL_HOST",
        "port": "DB_MYSQL_PORT",
        "user": "DB_MYSQL_USER",
        "password": "DB_MYSQL_PASS",
        "database": "DB_MYSQL_NAME",
        "authMechanism": "AUTH_MYSQL_MECHANISM",
        "authSource": "AUTH_MYSQL_SOURCE"
      }
    },
    "migrationsStoreTable":{
      "tableName": "MIGRATIONS_STORE",
      "tableId": "MIGRATIONS_STORE_ID"
    }
  },
  "app":{
    "automaticMigrationOperations": {
      "up": "MIGRATIONS_UP",
      "upWithOverride": "MIGRATIONS_UP_OVERRIDE",
      "down": "MIGRATIONS_DOWN",
      "downWithOverride": "MIGRATIONS_DOWN_OVERRIDE"
    },
    "migrationsDir": "MIGRATIONS_DIR",
    "operationsLabels": {
      "statusExecuted": "STATUS_EXECUTED",
      "statusRolledBack": "STATUS_ROLLED_BACK",
      "statusPending": "STATUS_PENDING",
      "rolledBackSuccess": "ROLLED_BACK_SUCCESS",
      "rolledBackFailed": "ROLLED_BACK_FAILED",
      "outcomeSuccess": "OUTCOME_SUCCESS",
      "outcomeFailed": "OUTCOME_FAILED",
      "outcomeMissing": "OUTCOME_MISSING"
    }
  },
  "log": {
    "name": "SERVICE_NAME",
    "base": {
      "env": "NODE_ENV"
    }
  }
}
