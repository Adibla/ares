{
  "db":  {
    "connection": {
      "MONGODB":{
        "host": "localhost",
        "port": 27888,
        "user": "mongoadmin",
        "password": "secret",
        "database": "test",
        "authMechanism": "DEFAULT",
        "authSource": "admin"
      },
      "MYSQL":{
        "host": "localhost",
        "port": 3306,
        "user": "root",
        "password": "password",
        "database": "app",
        "authMechanism": "DEFAULT",
        "authSource": "admin"
      }
    },
    "migrationsStoreTable":{
      "tableName": "migrations-store",
      "tableId": "id"
    },
    "seeds": {
      "directory": "./priv/seeds"
    }
  },
  "app":{
    "automaticMigrationOperations": {
      "up": "u",
      "upWithOverride": "uu",
      "down": "d",
      "downWithOverride": "dd"
    },
    "migrationsDir": "./bin/data",
    "operationsLabels": {
      "statusExecuted": "EXECUTED",
      "statusRolledBack": "ROLLED_BACK",
      "statusPending": "PENDING",
      "outcomeSuccess": "COMPLETE_SUCCESS",
      "rolledBackSuccess": "ROLLED_BACK_SUCCESS",
      "rolledBackFailed": "ROLLED_BACK_FAILED",
      "outcomeFailed": "COMPLETE_FAILED",
      "outcomeMissing": "NONE"
    }
  },
  "log": {
    "name": "app",
    "base": {
      "env": "test"
    },
    "enabled": false,
    "level": 30
  }
}
